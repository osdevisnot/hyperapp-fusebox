{
  "name": "hyperapp-fusebox",
  "version": "1.0.0",
  "description": "Starting Point for a Hyperapp and Picostyle projects.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .fusebox",
    "predev": "npm run clean",
    "dev": "node fuse dev",
    "preprod": "npm run clean",
    "prod": "node fuse prod",
    "predist": "npm run clean",
    "dist": "node fuse dist",
    "presetup": "git clean -fdX",
    "setup": "npm install",
    "precommit": "lint-staged",
    "post-rewrite": "npm install",
    "post-merge": "npm install"
  },
  "dependencies": {
    "@hyperapp/router": "0.7.1",
    "hyperapp": "1.2.5",
    "picostyle": "1.0.2"
  },
  "devDependencies": {
    "fuse-box": "3.2.2",
    "husky": "0.14.3",
    "lint-staged": "7.1.1",
    "npm-sort": "0.0.4",
    "prettier": "1.12.1",
    "tslib": "1.9.1",
    "typescript": "2.8.3",
    "uglify-es": "3.3.9"
  },
  "lint-staged": {
    "*.{js,css,json}": ["prettier --single-quote --no-semi --print-width 120 --write", "git add"],
    "*package.json": ["npm-sort", "git add"]
  }
}
